//search insertion position by binary search
#include <iostream>
using namespace std;
int main() {
    int arr[5]={3,5,6,8,9};
    int start=0,end=4,mid,key=5,ans;
    while(start<=end)
    {
        mid=(start+end)/2;
        if(arr[mid]==key){
             mid=ans;
            break;
        }
        else if(arr[mid]<key){
            start=mid+1;
        }
        else{
            mid=ans;
            end=mid-1;
        }
    }
    cout<<ans;
    return 0;
}
